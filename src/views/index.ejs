                            <input type="text" id="team" name="Club WP" class="form-control" placeholder="Equipo de waterpolo">
                          </div>
                          <div class="column" style="text-align: left;margin-left: 10px;">
                            <label for="position" class="form-label" >Posición de juego</label>
                            <input type="text" id="position" name="Posición" class="form-control" placeholder="Posición de juego">
                          </div>
                          </div>
                        </div>

