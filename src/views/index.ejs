<script>
  const scriptURL = 'https://script.google.com/macros/s/AKfycbw4CYkbsOf1YSbFAgGBnGirfsGQorMbjJX9Fi_DtIQc191Irgl_E-2GoKRxmnr51SX0/exec'
  const form = document.forms['google-sheet']

  form.addEventListener('submit', e => {
    e.preventDefault()
    fetch(scriptURL, { method: 'POST', body: new FormData(form)})
      .then(response => $("#form-alerts").html("<div class='alert alert-success'>¡Inscripción completada, para la realización del pago nos pondremos en contacto con ustedes! Os esperamos</div>"))
      .then(response =>  form.reset())
      .catch(error => console.error('Error!', error.message))
  })
</script>




